{
  "name": "iron-enum",
  "version": "1.1.1",
  "description": "Rust like enums for Typescript",
  "main": "dist/mod.js",
  "types": "dist/mod.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "minify": "npx terser dist/mod.js --compress --mangle --output dist/mod.min.js",
    "size": "gzip -c dist/mod.min.js | wc -c",
    "readme": "(echo \"/**\"; sed 's/^/ * /' README.md; echo \" */\") | cat - mod.ts > temp && mv temp mod.ts"
  },
  "author": "Scott Lott <me@scottlott.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/only-cliches/iron-enum"
  },
  "keywords": [
    "enum",
    "algebraic data type",
    "tagged union",
    "union",
    "rust enums",
    "rust",
    "option",
    "result",
    "rust enum",
    "rust like enum"
  ]
}
